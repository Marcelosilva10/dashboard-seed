<div class="row">
  <div class="col-xs-16">
    <div class="form-group">
      <label for="name">Nome</label>
      <input type="text" class="form-control" id="" placeholder="Insira seu nome" value="{{userLogged.name}}">
      <p class="help-block">Help text here.</p>
    </div>
    <div class="form-group">
      <label for="">E-mail</label>
      <input type="text" class="form-control" id="" placeholder="Insira E-mail" value="{{userLogged.local.email}}">
      <p class="help-block">Help text here.</p>
    </div>
    <div class="form-group">
      <label for="">URL personalizada</label>
      <input type="text" class="form-control" id="" placeholder="Escolha o nome">
      <p class="help-block">Help text here.</p>
    </div>
    <div class="form-group">
      <label for="Descrição">Sobre você</label>
      <textarea name="name" rows="6" class="form-control"></textarea>
      <p class="help-block">Help text here.</p>
    </div>
    <div class="form-group">
      <button class="btn btn-success btn-lg">Salvar alterações</button>
    </div>
  </div>
  <div class="col-xs-8">
    <div class="form-group avatar-edit">
      <label for="Descrição">Avatar</label>
      <div class="fileinput fileinput-new" data-provides="fileinput">
        <div class="fileinput-new thumbnail" style="width: 200px; height: 200px;">
          <img data-src="holder.js/200x200?theme=vine" alt="200 x 200px">
        </div>
        <div class="fileinput-preview fileinput-exists thumbnail" style="max-width: 200px; max-height: 200px;"></div>
        <div>
          <span class="btn btn-default btn-file"><span class="fileinput-new">Select image</span><span class="fileinput-exists">Change</span><input type="file" name="..."></span>
          <a href="#" class="btn btn-default fileinput-exists" data-dismiss="fileinput">Remove</a>
        </div>
      </div>
    </div>
  </div>
</div>
